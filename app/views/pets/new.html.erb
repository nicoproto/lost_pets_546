<%= form_for @pet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :species %>
  <%= f.select :species, collection: Pet::SPECIES %>

  <%= f.label :found_at %>
  <%= f.date_field :found_at %>

  <%= f.submit %>
<% end %>
